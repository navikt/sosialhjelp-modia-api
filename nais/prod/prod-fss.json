{
  "applicationName": "sosialhjelp-modia-api",
  "namespace": "teamdigisos",
  "ingress": [
    "https://sosialhjelp-modia-api.intern.nav.no/sosialhjelp/modia-api"
  ],
  "secretKvPaths": {
    "/var/run/secrets/nais.io/virksomhetssertifikat": "/secret/virksomhetssertifikat/prod/maskinporten"
  },
  "envsecret": [
    "redis-password-modia-api",
    "fiks-integrasjon-secret",
    "serviceuser-srvsosialhjelp-mod"
  ],
  "env": {
    "ABAC_PDP_ENDPOINT_URL": "https://abac-sosialhjelp.intern.nav.no/application/authorize",
    "FIKS_DIGISOS_ENDPOINT_URL": "https://api.fiks.ks.no",
    "ID_PORTEN_TOKEN_URL": "https://oidc.difi.no/idporten-oidc-provider/token",
    "ID_PORTEN_CONFIG_URL": "https://oidc.difi.no/idporten-oidc-provider/.well-known/openid-configuration",
    "ID_PORTEN_CLIENT_ID": "bd520e59-6e01-4669-a7c8-f68fa2cac362",
    "ID_PORTEN_SCOPE": "ks:fiks",
    "NORG_ENDPOINT_URL": "https://norg2.nais.adeo.no/norg2/api/v1",
    "NORG_OPPSLAG_URL": "https://norg2-frontend.nais.adeo.no/#/startsok?enhetNr=",
    "PDL_ENDPOINT_URL": "https://pdl-api.nais.adeo.no/graphql",
    "STS_TOKEN_ENDPOINT_URL": "https://security-token-service.nais.adeo.no/rest/v1/sts/token",
    "SPRING_PROFILES_ACTIVE": "prod-fss,log-kibana,log-audit",
    "CACHE_TIME_TO_LIVE_SECONDS": "180",
    "REDIS_HOST": "sosialhjelp-modia-api-redis.teamdigisos.svc.nais.local",
    "UNLEASH_API_URL": "https://unleash.nais.io/api",
    "UNLEASH_INSTANCE_ID": "prod-fss",
    "SWAGGER_ENABLED": "false"
  },
  "minReplicas": "2",
  "maxReplicas": "4",
  "cpuLimit": "500m",
  "memoryLimit": "1Gi",
  "cpuRequest": "10m",
  "memoryRequest": "512Mi",
  "prometheusEnabled": true,
  "webProxyEnabled": true,
  "isGcp": false,
  "isLabsGcp": false,
  "vaultEnabled": true,
  "dev": false,
  "azureEnabled": true
}
